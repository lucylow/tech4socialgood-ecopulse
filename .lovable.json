{
  "name": "ecopulse",
  "version": "1.0.0",
  "type": "nextjs",
  "framework": "next",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": "nextjs-app/.next",
  "publicDirectory": "nextjs-app/public",
  "sourceDirectory": "nextjs-app",
  "environmentVariables": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_NAME": "EcoPulse",
    "NEXT_PUBLIC_APP_VERSION": "1.0.0"
  },
  "aiFeatures": {
    "enabled": true,
    "ollamaIntegration": true,
    "model": "llama3.2:1b",
    "baseUrl": "http://127.0.0.1:11434"
  },
  "deployment": {
    "platform": "vercel",
    "autoDeploy": true,
    "branch": "main"
  }
}
